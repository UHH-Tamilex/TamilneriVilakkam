<text corresp="#KKms"><body xml:lang="ta">
    <milestone unit="page" n="30" facs="30"/>
    <pb n="30"/>
  <lg xml:id="TNV16poem9" corresp="TNV16poem9">
    <l>nōtaka vuṭaittē neñca matimicai</l>
<l>mātar kuvaḷai malarn ta<space quantity="2" unit="characters"/></l>
<l><space quantity="3" unit="characters"/> kaṇkaṇ ṭāṟṟiya</l>
<l>peruntakai yaṇṇalaik kaḻa<del rend="strikethrough">ṉ</del>ṟiṉaṉ peritē</l></lg>
<lg xml:id="TNV17poem5" corresp="TNV17poem5">  
<l>ampumukaṅ kiḻitta puṇvāy<add place="above">k</add> <subst><del rend="strikethrough">ta</del><add place="above">ka</add></subst>laimāṉ</l>
<l>pōntaṉa vuḷavō </l>
<l>maṭa ṉōkka marīi<add rend="caret" place="above">yi</add>ṉam paṭarntē</l></lg>  
  <lg xml:id="TNV17poem7" corresp="TNV17poem7">
  <l>kai<subst><del rend="strikethrough">vva</del><add place="above">ya</add></subst>tu cemmalan taḻaiyē viṉāvatu</l>
<l>puṇ vāya māvēl kaiviṭ</l>
<l>ṭakalā ṉammaviv vakaṉpuṉai</l>
<l>takaiyō ṉuḷḷiya taṟiyōm peritē</l></lg>
  <lg xml:id="TNV17poem35" corresp="TNV17poem35">
<l>tantunī yaḷitta taṇṭaḻai kāṇalum</l>
<l>vantaṉa ḷetirnta maṭantai neñca</l>
<l>maṇmicai viḷaṅkiya vaḻuttūr matitara</l>
<l><subst><del rend="strikethrough">ṟu</del><add place="in-place">ṉu</add></subst>ṇṇiyaṟ ṟamiḻi ṉuḻaiporu tuḷitta</l>
<l>vā<subst>y<del rend="puḷḷi">a</del><add rend="caret" place="above">moḻi</add></subst>miḻta maṭuttavar maṉameṉa</l>
<l>yāṉilai peṟṟaṉ ṟiyāṉaṟin tilaṉē</l></lg>
    <lg xml:id="TNV17poem38" corresp="TNV17poem38">
<l>pē<del rend="brackets">y</del>tuṟal vāḻiniṉ kātaṟ ṟōḻi</l>
<l>kaipuṉai kōla <add place="above" rend="caret">me</add>ṉaypeṟ ṟaṉṟē</l>
<l>yaiya māvatu miṉṟip</l>
<l>peypūṅ kōtai peruṅkaviṉ koḷavē</l></lg>
   <lg xml:id="TNV17poem40" corresp="TNV17poem40">
<l>naṟumpūṅ kaṇṇiyum peruntaṇ kōtaiyum</l>
<l>na<subst><del rend="strikethrough">tai</del><add place="above">kai</add></subst>vāyp piṇaiyalu mukaivāyc cūṭṭum</l>
<l>puṉaintaṉai yaruḷal vēṇṭuñ</l>
<l>ciṉaṅkeḻu kāṉavaṉ ceḻumaṭa makaḷē.</l></lg>
   <lg xml:id="TNV18poem11" corresp="TNV18poem11">
<l>varivaḷait tōḻi yorutaṉi yoḻiyap</l>
<l>piritu meṉṟi yāyiṟ peritaḻin</l>
<l>talamaral vāḻi yaiya</l>
     <l>viruḷiṭai yeytalu <subst><del rend="overwritten">ma</del><add place="in-place">me</add></subst>matumā ṉamakkē</l></lg>
  <lg xml:id="TNV18poem18" corresp="TNV18poem18">
<l>puḷḷun tuyiṟpuṭai peyar<add rend="caret" place="above">n</add>taṉa puṉaluḷ</l>
<l>veḷḷita<subst><del rend="strikethrough">ṭ</del><add place="above">ḻk</add></subst> kaitai maṇikkāy tatumpa</l>
<l>vantaṉaṉ kollō tāṉē</l>
<l>ventiṟa laṇṇa ṉiṉaintaṉaṉ viraintē</l></lg>
  <lg xml:id="TNV18poem19" corresp="TNV18poem19">
<l>aṉṉāy kāṇeṉ ṟirumuṟai taṉṉai</l>
<l>yeṭuppavun tuyiṉmaṭin tilaḷē</l>
<l>yilaṅkilai neṭuvē laṇṇalum</l>
<l>pulamputuya rakalak karuti vantōṉē</l></lg>
   <lg xml:id="TNV18poem22" corresp="TNV18poem22">
<l>vaṇarcuri yaimpāl vāṇuta larivai</l>
<l>yavaṭiṟa maṟital yāvatum</l>
     <l>puṇartuyiṉ maṟantaṉa<del rend="strikethrough">m</del> puḷḷiṉam peyarntē</l></lg>
  <lg xml:id="TNV19poem9" corresp="TNV19poem9">
<l>itaṟkoṇ ṭiyāṉ ṟe<subst><del rend="strikethrough">ḻi</del><add place="above">ḷi</add></subst>ntu mēṉāḷ</l>
<l>matikkō ṭuñcu mālvarai vāṭkaik</l>
<l>kaṭavu ḷāka<del rend="strikethrough">la</del> mallatum</l>
<l>maṭavaraṉ mātarai matittaṉṟu milamē</l></lg>
<lg xml:id="TNV20poem5" corresp="TNV20poem5">
  <milestone unit="page" n="114" facs="61"/>
<l>tīyiṉum veṉkuvai yāyiṉ</l>
<l>yāvatu miṉiya kā<add rend="caret" place="above">ṉa</add>m</l>
<l>cēyuyar ci<subst><del rend="strikethrough"><unclear>lam</unclear></del><add place="above">lampa</add></subst> niṉṉoṭu celiṉē</l></lg>  
    <pb n="114"/>
</body>

